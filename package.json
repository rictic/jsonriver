{
  "name": "jsonriver",
  "type": "module",
  "version": "1.0.0",
  "description": "A JSON parser that produces increasingly complete versions of the parsed value.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "license": "BSD-3-Clause",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "start": "wds --watch",
    "format": "prettier . -w",
    "format:check": "prettier . -c"
  },
  "keywords": [
    "json",
    "parser",
    "streaming",
    "incremental"
  ],
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@web/dev-server": "^0.4.6",
    "esbuild": "^0.24.0",
    "eslint": "^8.48.0",
    "openai": "^4.67.3",
    "prettier": "^3.3.3",
    "stream-json": "^1.8.0",
    "tachometer": "^0.7.1",
    "typescript": "^5.6.3",
    "wireit": "^0.14.9"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false
  }
}
